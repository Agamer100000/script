local content = createGUI()

createSection(content, "üí∞ MONEY")
createToggle(content, "Infinite Yen", toggleInfiniteYen)
createButton(content, "üíé Max Upgrades (999)", maxUpgrades)

createSection(content, "‚öîÔ∏è COMBAT")
createToggle(content, "Enhanced God Mode (3 Methods)", toggleGodMode)
createToggle(content, "Bigger Enemy Hitbox", toggleBiggerHitbox)
createButton(content, "Hitbox Size: 10", function() Settings.HitboxSize = 10 end)
createButton(content, "Hitbox Size: 20 (Large)", function() Settings.HitboxSize = 20 end)
createButton(content, "Hitbox Size: 30 (Huge)", function() Settings.HitboxSize = 30 end)
createButton(content, "üîì Unlock All Powers", unlockAllPowers)

createSection(content, "üèÉ MOVEMENT")
createToggle(content, "Speed Hack", toggleSpeedHack)
createButton(content, "Speed: 50", function() setWalkSpeed(50) end)
createButton(content, "Speed: 100", function() setWalkSpeed(100) end)
createButton(content, "Speed: 200", function() setWalkSpeed(200) end)
createToggle(content, "Auto Dash", toggleAutoDash)
createToggle(content, "Infinite Jump", toggleInfiniteJump)

createSection(content, "üëÅÔ∏è VISUAL & MONITORING")
createToggle(content, "Player ESP (Name, Health, Distance)", toggleESP)
createToggle(content, "Live Monitoring Display", toggleMonitoring)

print("‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ")
